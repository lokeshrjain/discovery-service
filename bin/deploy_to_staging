#!/usr/bin/env bash

set -e

ECS_SERVICE_NAME='discovery-service'
ECS_TASK_DEFINITION_NAME='discovery-service'
ECR_NAME='shopalitic/discovery-service'


DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"
(
  echo ${DIR}
  cd "${DIR}/.."
  ./bin/deploy -e staging -b origin/master -s ${ECS_SERVICE_NAME} -t ${ECS_TASK_DEFINITION_NAME} -r ${ECR_NAME}
)

